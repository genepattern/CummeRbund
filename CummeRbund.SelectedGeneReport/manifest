p4_fileFormat=
p4_name=output.format
p4_optional=
p4_prefix_when_specified=
p4_type=java.lang.String
p4_value=pdf;svg;png

p5_default_value=genes
p5_description=Feature level for the report.  Note that the Similarity plots will always be generated at the "genes" level.
p5_fileFormat=
p5_name=feature.level
p5_optional=
p5_prefix_when_specified=
p5_type=java.lang.String
p5_value=genes\=genes;isoforms\=isoforms;TSS\=transcription start site (TSS);CDS\=coding sequence (CDS)

p6_default_value=yes
p6_description=Controls whether reporting should be done for individual replicates or aggregate condition/sample values.  The default is to use aggregate sample values.  Note that the Similarity plots always show aggregate samples.
p6_fileFormat=
p6_name=report.as.aggregate
p6_optional=
p6_prefix_when_specified=
p6_type=java.lang.String
p6_value=yes;no

