name=CummeRbund.SelectedGeneReport
commandLine=<R2.15_HOME>/bin/Rscript --no-save --quiet --slave --no-restore <libdir>run_gp_cummerbund_selected_gene_report.R <libdir> <patches>/rlib/2.15_20131119/site-library --cuffdiff.input\=<cuffdiff.input> --feature.id=<feature.id> <find.similar> <reference.GTF> <genome> --output.format=<output.format> --feature.level=<feature.level> --report.as.aggregate=<report.as.aggregate> --log.transform=<log.transform>
license=
taskDoc=doc.html
fileFormat=
taskType=RNA-seq
os=any
privacy=public
description=Cuffdiff visualization package providing plots based on a single user-specified gene.
version=Minor parameter description fixes based on doc review.
minGenePatternVersion=
author=Loyal Goff, MIT Computer Science and Artificial Intelligence Lab

p1_default_value=
p1_description=A Cuffdiff job, a previous CummeRbund job, or a cuffData.db file from a previous CummeRbund job.
p1_fileFormat=directory;db
p1_MODE=IN
p1_name=cuffdiff.input
p1_optional=
p1_prefix_when_specified=
p1_TYPE=FILE
p1_type=java.io.File
p1_value=

p2_default_value=
p2_description=The gene or feature of interest.  This can be a gene symbol (short name), gene ID, isoform_id, tss_group_id, or cds_id.
p2_fileFormat=
p2_name=feature.id
p2_optional=
p2_type=java.lang.String
p2_value=

p3_default_value=10
p3_description=Optionally, find and plot the top genes (up to this count) with an expression profile most similar to the given gene of interest.  If blank, this will be skipped.    
p3_fileFormat=
p3_flag=--find.similar\=
p3_name=find.similar
p3_optional=on
p3_prefix=--find.similar\=
p3_prefix_when_specified=--find.similar\=
p3_type=java.lang.Integer
p3_value=

p4_MODE=IN
p4_TYPE=FILE
p4_choiceDir=ftp\://gpftp.broadinstitute.org/module_support_files/annotation/gtf/
p4_choiceDirFilter=*.gtf;*.GTF
p4_default_value=
p4_description=An optional but highly recommended reference annotation GTF. This should be the same reference used in the original Cuffdiff job.
p4_fileFormat=gtf
p4_flag=--ref.gtf\=
p4_name=reference.GTF
p4_optional=on
p4_prefix=--ref.gtf\=
p4_prefix_when_specified=--ref.gtf\=
p4_type=java.io.File
p4_value=

p5_default_value=
p5_description=The name of the genome used in the original Cuffdiff job (e.g. 'hg19, 'mm9').  You should not need to change this unless you are providing your own reference GTF.
p5_fileFormat=
p5_flag=--genome\=
p5_name=genome
p5_optional=on
p5_prefix=--genome\=
p5_prefix_when_specified=--genome\=
p5_type=java.lang.String
p5_value=

p6_default_value=pdf
p6_description=The output file format.
p6_fileFormat=
p6_name=output.format
p6_optional=
p6_prefix_when_specified=
p6_type=java.lang.String
p6_value=pdf;svg;png

p7_default_value=genes
p7_description=Feature level for the report.  Note that the Similarity plots will always be generated at the "genes" level.
p7_fileFormat=
p7_name=feature.level
p7_optional=
p7_prefix_when_specified=
p7_type=java.lang.String
p7_value=genes\=genes;isoforms\=isoforms;TSS\=transcription start site (TSS);CDS\=coding sequence (CDS)

p8_default_value=yes
p8_description=Controls whether reporting should be done for individual replicates or aggregate condition/sample values.  The default is to use aggregate sample values.  Note that the Similarity plots always show aggregate samples.
p8_fileFormat=
p8_name=report.as.aggregate
p8_optional=
p8_prefix_when_specified=
p8_type=java.lang.String
p8_value=yes;no

p9_default_value=yes
p9_description=Whether or not to log transform the FPKM values.
p9_fileFormat=
p9_name=log.transform
p9_optional=
p9_prefix_when_specified=
p9_type=java.lang.String
p9_value=yes;no

