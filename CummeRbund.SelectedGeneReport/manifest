p1_value=

p2_default_value=
p2_description=The gene or feature of interest.  This can be a gene symbol (short name), gene ID, isoform_id, tss_group_id, or cds_id.
p2_fileFormat=
p2_name=feature.id
p2_optional=
p2_type=java.lang.String
p2_value=

p3_default_value=10
p3_description=Optionally, find and plot the top genes (up to this count) with an expression profile most similar to the given gene of interest.  If blank, this will be skipped.    
p3_fileFormat=
p3_flag=--find.similar\=
p3_name=find.similar
p3_optional=on
