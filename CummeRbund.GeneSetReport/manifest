p4_default_value=genes
p4_description=Feature level for the report.
p4_fileFormat=
p4_name=feature.level
p4_optional=
p4_prefix_when_specified=
p4_type=java.lang.String
p4_value=genes\=genes;isoforms\=isoforms;TSS\=transcription start site (TSS);CDS\=coding sequence (CDS)

p5_default_value=yes
p5_description=Controls whether reporting should be done for individual replicates or aggregate condition/sample values.  The default is to use aggregate sample values.  Note that the Volcano plot always shows aggregate samples.
p5_fileFormat=
p5_name=report.as.aggregate
p5_optional=
p5_prefix_when_specified=
p5_type=java.lang.String
p5_value=yes;no

p6_default_value=yes
p6_description=Whether or not to log transform the FPKM values.  Note that the FPKM values are always log transformed for the Volcano plots and that the Scatter plot will use a log2 rather than log10 transformation.  For the Expression Plot and Expression Barplot, this directs that the y-axis will be drawn on a log10 scale.
p6_fileFormat=
p6_name=log.transform
p6_optional=
p6_prefix_when_specified=
p6_type=java.lang.String
p6_value=yes;no

p7_default_value=
p7_description=An optional count for the K-Means clustering plot; the clustering algorithm will assign genes (or lower-level features) to this number of clusters. If blank, this plot will be skipped.
p7_fileFormat=
p7_flag=--cluster.count\=
p7_name=cluster.count
p7_optional=on
p7_prefix=--cluster.count\=
